INSERT [dbo].[Movie] ([Id], [Picture], [Name], [Description], [Duration]) VALUES (2, N'2', N'B\f1\lang1066\u7840?N H\u7884?C C\f0\lang1033\'d5I \'c2M', N'Nh\f1\lang1066\u7919?ng ng\f0\lang1033\'e0y \f2\'f0\f1\lang1066\u7847?u ti\f0\lang1033\'ean t\f1\lang1066\u7841?i tr\f2\u432?\f1\u7901?ng trung h\u7885?c c\u7911?a c\u7853?u nam sinh Alex ch\u7859?ng kh\f0\lang1033\'e1c g\'ec \f2\'f0\f1\lang1066\u7883?a ng\u7909?c, khi n\u7841?n b\u7855?t n\u7841?t v\f0\lang1033\'e0 b\f1\lang1066\u7841?o l\u7921?c h\u7885?c \f2\'f0\u432?\f1\u7901?ng di\u7877?n ra nh\f2\u432? c\u417?m b\f1\u7919?a. S\u7921? xu\u7845?t hi\u7879?n c\u7911?a c\f0\lang1033\'f4 b\f1\lang1066\u7841?n Maggie khi\u7871?n cho cu\u7897?c s\u7889?ng c\u7911?a Alex th\f0\lang1033\'fa v\f1\lang1066\u7883? h\f2\u417?n h\f1\u7859?n. C\u7843? hai s\u7899?m tr\u7903? th\f0\lang1033\'e0nh \f2\'f0\f0\'f4i b\f1\lang1066\u7841?n th\f0\lang1033\'e2n. Tuy nhi\'ean, nh\f1\lang1066\u7919?ng s\u7921? vi\u7879?c k\f0\lang1033\'ec l\f1\lang1066\u7841? v\f0\lang1033\'e0 kinh ho\'e0ng b\f1\lang1066\u7855?t \f2\'f0\f1\u7847?u x\u7843?y ra. \f2\'d0\f0\lang1033\'e3 c\'f3 nh\f1\lang1066\u7919?ng c\f0\lang1033\'e1i ch\f1\lang1066\u7871?t \f2\'f0\u432?\f1\u7907?c g\f0\lang1033\'e2y ra b\f1\lang1066\u7903?i nh\u7919?ng \ldblquote b\u7841?n h\u7885?c c\f0\lang1033\'f5i \'e2m\rdblquote  khi\f1\lang1066\u7871?n cho ng\f0\lang1033\'f4i tr\f2\u432?\f1\lang1066\u7901?ng tr\u7903? n\f0\lang1033\'ean \f2\'f0\f0\'e1ng s\f1\lang1066\u7907? h\f2\u417?n bao gi\f1\u7901? h\u7871?t.', 90)\par
